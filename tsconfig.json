{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vite",
  "compilerOptions": {
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "allowJs": true,
    "module": "ESNext",
    "target": "es2016",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "incremental": false,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "types": [
      "vite/client"
    ]
  },
  "exclude": [
    "dist",
    "node_modules",
    "app/assets/config",
    "./*.config.js"
  ]
}
